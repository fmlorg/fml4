### internal variables
init=0
package=0
release=0
branch=0

### user defined variables

# prog
MAKE=make
INCLUDE_PROG="perl distrib/bin/_include.pl "

FWIX_OPTS=-F

# working directory
UPDIR=/var/tmp

TMP=/var/tmp/release.info.$$ 

BAKID=`date +%y%m%d.%H%M%S`

# log file
DATE_LOG=/var/tmp/_datelog_

# config file

PATCHLEVEL_DIR=distrib/patch
PATCHLEVEL_FILE=$PATCHLEVEL_DIR/version

# distribution list
RELEASE_MANIFEST=etc/RELEASE_FILES

# distribution directory of snapshot
DIRECTORY_LIST=distrib/etc/DIRECTORY_LIST


perl distrib/bin/gen_variable_from_release_files.pl distrib/etc/RELEASE_FILES > $TMP
. $TMP


###### check 
if [ "X" = "X$UPDIR" ]
then
	echo " "; echo "No set UPDIR, exit"
	exit 1
else
	echo $UPDIR	
fi

