#!/bin/sh

echo " "
sh distrib/bin/make-fmllocal.man.sh

echo "Makeing var/doc/op"

(cd var/doc; $FML/distrib/bin/version.pl)

perl distrib/bin/fix-wix.pl -X $FML doc/smm/op.wix |\
perl bin/fwix.pl -M var/tmp/MANIFEST -d doc/smm > var/doc/op.jp

perl distrib/bin/fix-wix.pl -X $FML doc/smm/op.wix |\
perl bin/fwix.pl -M var/tmp/MANIFEST -d doc/smm -L ENGLISH > var/doc/op.en

exit 0;
